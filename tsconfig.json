{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2017",
    "declaration": false,
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictPropertyInitialization": false,    
    "forceConsistentCasingInFileNames": true, 
    "strictNullChecks": true
  },
  "lib": ["ESNext", "DOM" ]
}


